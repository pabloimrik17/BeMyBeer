include header.jade



div
    label(for="username") Nombre
        input(type="text",name="username", ng-model="username")
        br
    label(for="email") Email
        input(type="text",name="email" ng-model="email")
        br
    label(for="password") Password
        input(type="text",name="password" ng-model="password")
        br

    button.btn.btn-default(ng-click="clickButton()") Click

div
    ul(ng-repeat="item in response.data")
        li Name:{{item.username}}, Email: {{item.email}}, Registration Date: {{item.created_at | date : 'medium'}}
