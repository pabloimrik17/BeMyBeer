{
    "env": {
        "mocha": true,
        "node": true
    },
    "extends": "airbnb-base",
    "rules": {
        "linebreak-style": ["error", "windows"],
        "indent": ["error", 4],
        "no-underscore-dangle": ["off"]
    },
    "parser": "typescript-eslint-parser",
    "plugins": [
        "typescript"
    ]
}